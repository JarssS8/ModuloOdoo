<odoo>
    <data>
        <record model="ir.ui.view" id="detaileduser_form_view">
            <field name="name">detaileduser.form</field>
            <field name="model">usersmanagement.premium</field>
            <field name="inherit_id" ref="res.partner.form">
            <field name="arch" type="xml">
                <xpath expr="//label[@string='Address']" position="before">
                    <field name="login" string="Login"/>
                    <field name="userType" string="User type"/>
                    <field name="premiumCount" string="Months subscribed"/>
                    <field name="autorenovation" string="Autorenovation active"/>
                    <field name="documents.totalDownloads" string="Total Downloads"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
